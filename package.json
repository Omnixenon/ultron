{
  "name": "@omnixenon/ultron",
  "version": "0.0.0",
  "description": "Ultron, a fully featured React component library",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "rollup": "rollup --config config/rollup/rollup.config.ts --configPlugin typescript",
    "storybook": "start-storybook -p 6006 -c ./config/storybook --no-manager-cache",
    "build:storybook": "build-storybook -c ./config/storybook",
    "prettier:write": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prettier:format": "prettier --check \"src/**/*.{ts,tsx}\"",
    "eslint:write": "eslint --fix src --ext .ts,.tsx",
    "eslint:format": "eslint src --ext .ts,.tsx",
    "stylelint:write": "stylelint --fix './src/**/*.styles.{ts,tsx}'",
    "stylelint:format": "stylelint './src/**/*.styles.{ts,tsx}'",
    "test:jest": "jest -c config/jest/jest.config.ts",
    "lint:write": "pnpm eslint:write && pnpm prettier:write && pnpm prettier:write",
    "lint:format": "pnpm eslint:format && pnpm prettier:format && pnpm prettier:format",
    "lint": "pnpm lint:format && pnpm lint:write",
    "test": "pnpm test",
    "watch": "pnpm storybook",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Omnixenon/ultron.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Omnixenon/ultron/issues"
  },
  "homepage": "https://github.com/Omnixenon/ultron#readme",
